{
  "name": "scandipwa-storybook-plugin",
  "version": "0.1.0-rc.0",
  "license": "OSL-3.0",
  "private": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/plato2/foundry/app/scandipwa-storybook"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@craco/craco": "^6.1.2",
    "@scandipwa/eslint-config": "0.1.25",
    "@scandipwa/scandipwa": "4.4.0",
    "@scandipwa/scandipwa-scripts": "2.4.8",
    "@tilework/mosaic": "0.1.2",
    "esserializer": "^1.0.6",
    "npm": "^7.19.1",
    "react-scripts": "^4.0.3",
    "serialijse": "^0.3.0",
    "serialize-javascript": "^6.0.0",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "scandipwa": {
    "type": "theme",
    "locales": {
      "en_US": true
    },
    "parentTheme": "@scandipwa/scandipwa"
  },
  "scripts": {
    "start": "scandipwa-scripts start",
    "build": "scandipwa-scripts build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "create-package": "rm -rf dist && NODE_ENV=production babel lib --out-dir dist --copy-files",
    "postinstall": "node lib/postinstall.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "@scandipwa",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "proxy": "https://40kskudemo.scandipwa.com/",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@tilework/mosaic-config-injectors": "0.1.7",
    "@tilework/mosaic-craco": "0.0.3",
    "@tilework/mosaic-dev-utils": "0.1.2",
    "@tilework/mosaic-test-utils": "0.0.15",
    "@tilework/mosaic-webpack-fallback-plugin": "0.0.8",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-rebem-jsx": "^0.3.3",
    "babel-preset-react-app": "^10.0.0",
    "chokidar": "^3.5.1",
    "craco-sass-resources-loader": "^1.1.0",
    "cross-spawn": "^7.0.0",
    "debounce": "^1.2.0",
    "jest": "26.6.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "4.14.1",
    "postcss": "^8.3.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "4.0.3",
    "precss": "^4.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-dev-utils": "^11.0.4",
    "resolve": "^1.20.0",
    "sass-loader": "10.1.1",
    "sass-resources-loader": "^2.2.2",
    "storybook-preset-craco": "^0.0.4",
    "tree-kill": "^1.2.2"
  }
}
